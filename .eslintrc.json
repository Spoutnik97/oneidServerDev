{
    "extends": "airbnb",
    "rules": {
      "func-names": 0,
      "linebreak-style": 0,
      "one-var": 0,
      "no-unused-vars": 1,
      "camelcase": 0,
      "no-console": 0,
      "jsx-filename-extension": 0,
      "consistent-return": 0,
      "max-len": 0,
      "no-plusplus": 0
    }
}
